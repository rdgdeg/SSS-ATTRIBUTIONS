{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/app-cours-vacants-complete.html"
    },
    {
      "source": "/app",
      "destination": "/app-cours-vacants-complete.html"
    },
    {
      "source": "/gestion",
      "destination": "/app-gestion-cours-vacants.html"
    },
    {
      "source": "/import",
      "destination": "/app-import-cours-vacants.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
